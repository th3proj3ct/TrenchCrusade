            <characteristic name="Ranged" typeId="1a4f-d2cd-c405-6200">+1</characteristic>
            <characteristic name="Melee" typeId="12e8-ff8c-c933-9eb5">+2</characteristic>
            <characteristic name="Armour" typeId="5de9-d70e-9021-6f71">-1</characteristic>
